{
  "name": "ci/jobs",
  "description": "",
  "license": "MIT",
  "require": {
    "illuminate/support": "^10.0",
    "jenssegers/agent": "^2.6",
    "stem/core": "dev-main"
  },
  "require-dev": {
    "stem/dev-standards": "dev-main"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:stetind/stem-dev-standards.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:stetind/stem-core.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ci\\Jobs\\": "src/",
      "Ci\\Jobs\\Database\\Factories\\": "database/factories/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ci\\Jobs\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "stem/dev-standards": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ci\\Jobs\\PackageProvider"
      ]
    }
  }
}
